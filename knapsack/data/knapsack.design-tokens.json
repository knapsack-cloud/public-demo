          "$type": "color",
          "$extensions": {
            "cloud.knapsack": {
              "group": {
                "collectionInfo": {
                  "type": "var"
                }
              }
            }
          },
          "arcadiaLight": {
            "$value": "#7A34ED"
          },
          "arcadiaDark": {
            "$value": "#7A34ED"
          },
          "basisLight": {
            "$value": "#ED7734"
