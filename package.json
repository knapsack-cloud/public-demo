{
  "name": "@knapsack-cloud/sandbox-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Knapsack Sandbox Starter",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rm -rf ./ks-public/dist ./dist",
    "ui:watch": "yarn ui:build -w",
    "ui:build": "rollup -c ./rollup.config.js",
    "build": "NODE_ENV=production yarn ui:build && yarn ks:build",
    "ks:build": "NODE_ENV=production knapsack build",
    "ks:serve": "NODE_ENV=production KNAPSACK_PORT=${PORT} knapsack serve",
    "ks:test": "knapsack test",
    "ks:start": "knapsack start",
    "prep": "[ -f ./ks-public/dist/index.css ] || yarn ui:build",
    "start": "yarn prep && run-p *:watch ks:start",
    "watch": "run-p ui:watch",
    "test": "run-s --print-name *:test"
  },
  "dependencies": {
    "http-proxy-middleware": "^1.0.5",
    "browser-sync": "^2.26.10",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@knapsack/app": "^3.4.1",
    "@knapsack/babel-config": "^3.4.1",
    "@knapsack/build-tools": "^3.4.1",
    "@knapsack/core": "^3.4.1",
    "@knapsack/plugin-changelog-md": "^3.4.1",
    "@knapsack/renderer-react": "^3.4.1",
    "@knapsack/renderer-twig": "^3.4.1",
    "@knapsack/renderer-vue": "^3.4.1",
    "@knapsack/renderer-web-components": "^3.4.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "autoprefixer": "^9.8.0",
    "classnames": "^2.2.6",
    "daemonite-material": "^4.1.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "lit-element": "^2.2.1",
    "lodash.template": "^4.4.0",
    "node-sass-magic-importer": "^5.3.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.16.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-styles": "^3.8.2",
    "rollup-plugin-terser": "^6.1.0",
    "sass": "^1.26.7",
    "style-dictionary": "^2.7.0",
    "typescript": "^3.9.5",
    "wait-on": "^5.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=10.0.0 || >=12.0.0"
  },
  "repository": "git://github.com/knapsack-cloud/sandbox-starter.git"
}
