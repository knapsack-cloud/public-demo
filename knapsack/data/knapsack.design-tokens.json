        "logo": {
          "$type": "string",
          "$extensions": {
            "cloud.knapsack": {
              "group": {
                "collectionInfo": {
                  "type": "var"
                }
              }
            }
          },
          "arcadiaLight": {
            "$value": "url('https://knapsack.imgix.net/site/ks-sandbox/logo-arcadia-4id5a3sy3ysvg')"
          },
          "arcadiaDark": {
            "$value": "url('https://knapsack.imgix.net/site/ks-sandbox/logo-arcadia-4id5a3sy3ysvg')"
          },
          "basisLight": {
