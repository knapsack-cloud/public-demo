          "$extensions": {
            "cloud.knapsack": {
              "group": {
                "collectionInfo": {
                  "type": "var"
                }
              }
            }
          },
          "arcadiaLight": {
            "$value": "#FFFFFF"
          },
          "arcadiaDark": {
            "$value": "#3B3644"
          },
          "basisLight": {
            "$value": "#FFFFFF"
          },
          "basisDark": {
            "$value": "#3B3644"
          }
        },
        "page": {
          "$type": "color",
          "$extensions": {
            "cloud.knapsack": {
