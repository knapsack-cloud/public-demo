              "cloud.knapsack": {
                "group": {
                  "collectionInfo": {
                    "type": "var-ref",
                    "referencedId": "collections.theme.brand.color"
                  }
                }
              }
            }
          }
        },
        "text": {
          "$type": "color",
          "$extensions": {
            "cloud.knapsack": {
              "group": {
                "collectionInfo": {
                  "type": "var"
                }
              }
            }
          },
          "arcadiaLight": {
            "$value": "{color.white}"
          },
          "arcadiaDark": {
            "$value": "{color.white}"
          },
          "basisLight": {
            "$value": "{color.white}"
          },
          "basisDark": {
            "$value": "{color.white}"
          }
        }
      },
